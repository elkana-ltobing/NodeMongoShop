<a class="addProduct btn btn-info" href="/list">Product Table</a>
<a class="addProduct btn btn-primary" href="/addNewProduct">Add New Product</a>

{{# each products }}
<div class="row">
    {{# each this }}
    <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
            <img src="{{ this.imagePath }}" alt="..." class="img-responsive">
            <div class="caption">
                <h3>{{ this.title }}</h3>
                <p class="description">{{ this.description }}</p>
                <div class="clearfix">
                    <a href="/add-to-cart/{{this._id}}" class="btn btn-success pull-right" role="button">Add to Cart</a>
                    <div class="price pull-left">IDR {{ this.price }}</div>
                    {{!-- <a href="#" class="btn btn-success pull-right" role="button">Add to cart</a> --}}
                    {{!-- <form action="/likes/{{this._id}}" method="get"></form> --}}
                    {{!-- <button class="btn btn-primary pull-right" type="submit">Likes</button> --}}
                    {{!-- </form> --}}
                    <a href="/likes/{{this._id}}" class="btn btn-primary pull-right" role="button">Like</a>
                </div>
            </div>
        </div>
    </div>
    {{/each}}
</div>
{{/each}}